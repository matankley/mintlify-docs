{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Hunch.dev Docs",
  "colors": {
    "primary": "#6366F1",
    "light": "#A5B4FC",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "integrations"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Hunch Home",
          "href": "https://hunch.dev",
          "icon": "house"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@hunch.dev",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/hunch-light.svg",
    "dark": "/logo/hunch-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Sign In",
        "href": "https://app.hunch.dev"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Try Hunch",
      "href": "https://app.hunch.dev/signup"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/hunchdev",
      "github": "https://github.com/hunch-dev",
      "linkedin": "https://linkedin.com/company/hunch-dev"
    }
  }
